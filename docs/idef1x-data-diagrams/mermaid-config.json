{
  "theme": "dark",
  "themeVariables": {
    "darkMode": true,
    "background": "transparent",
    "primaryColor": "#3b82f6",
    "primaryTextColor": "#ffffff",
    "primaryBorderColor": "#1e40af",
    "lineColor": "#6b7280",
    "secondaryColor": "#10b981",
    "tertiaryColor": "#f59e0b",
    "c0": "#1f2937",
    "c1": "#374151",
    "c2": "#4b5563",
    "c3": "#6b7280"
  },
  "flowchart": {
    "nodeSpacing": 50,
    "rankSpacing": 50,
    "curve": "basis",
    "padding": 20,
    "useMaxWidth": true,
    "htmlLabels": true
  },
  "sequence": {
    "diagramMarginX": 50,
    "diagramMarginY": 10,
    "boxTextMargin": 5,
    "noteMargin": 10,
    "messageMargin": 35,
    "mirrorActors": true
  },
  "gantt": {
    "titleTopMargin": 25,
    "barHeight": 20,
    "fontsize": 11,
    "sidePadding": 75,
    "leftPadding": 75,
    "gridLineStartPadding": 35,
    "fontSize": 11,
    "numberSectionStyles": 4
  }
